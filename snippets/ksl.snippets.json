{
	"Library Declaration": {
		"prefix": "using",
		"body": "using ${1|io,fs,math,test|};",
		"description": "Include a library"
	},
    "Function Declaration": {
        "prefix": "function",
        "body": [
			"function ${1}(${2}) {",
			"    ${3}",
			"    return${4};",
			"}"
		],
        "description": "New function declaration"
    },
	"If Statement": {
		"prefix": "if",
		"body": [
			"if (${1}) {",
			"    ${2}",
			"}"
		],
		"description": "New if statement"
	},
	"Else If Statement": {
		"prefix": "else",
		"body": [
			"else if (${1}) {",
			"    ${2}",
			"}"
		],
		"description": "Extend an if statement"
	},
	"Else Statement": {
		"prefix": "else",
		"body": [
			"else {",
			"    ${1}",
			"}"
		],
		"description": "Extend an if statement"
	},
	"For Loop": {
		"prefix": "for",
		"body": [
			"for (int ${1} = ${2}; ${3}; ${4};) {",
			"    ${5}",
			"}"
		],
		"description": "New for loop"
	}
}
